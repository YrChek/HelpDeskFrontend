{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-newline": ["error", { "consistent": true }],
        "no-console": ["error", { "allow": ["dir", "log"] }],
        "no-param-reassign": ["error", { "props": false }],
        "no-use-before-define": ["error", {
            "functions": false
        }],
        "operator-linebreak": ["error", "after"]
    }
}
